{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ymaps3-angular": {
      "projectType": "library",
      "root": "projects/ymaps3-angular",
      "sourceRoot": "projects/ymaps3-angular/src",
      "prefix": "ymap",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ymaps3-angular/ng-package.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
